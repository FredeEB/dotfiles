!Dracula Xresources palette
*.foreground: #F8F8F2
*.background: #282A36
*.color0:     #000000
*.color8:     #4D4D4D
*.color1:     #FF5555
*.color9:     #FF6E67
*.color2:     #50FA7B
*.color10:    #5AF78E
*.color3:     #F1FA8C
*.color11:    #F4F99D
*.color4:     #BD93F9
*.color12:    #CAA9FA
*.color5:     #FF79C6
*.color13:    #FF92D0
*.color6:     #8BE9FD
*.color14:    #9AEDFE
*.color7:     #BFBFBF
*.color15:    #E6E6E6


*.cursorColor: #ffffff
*.font: xft:Iosevka Nerd Font:style=Regular:pixelsize=13
*.boldFont: xft:Iosevka Nerd Font:style=Bold:pixelsize=13
*.italicFont: xft:Iosevka Nerd Font:style=Italic:pixelsize=13
*.boldItalicFont: xft:Iosevka Nerd Font:style=Bold Italic:pixelsize=13
*.letterspacing: -1
*.perl-ext-common: default,52-osc,clipboard,-confirm-paste
*.scrollBar: false
*.iso14755: false
*.iso14755_52: false
*.keysym.Shift-Control-V: eval:paste_clipboard
*.keysym.Shift-Control-C: eval:selection_to_clipboard
*.keysym.Control-Meta-c: builtin-string:
*.keysym.Control-Meta-v: builtin-string:

! ROFI Color theme
rofi.theme: ~/.config/rofi/rofi.conf

rofi.terminal: urxvt
rofi.run-command: zsh -c "{cmd}"
